<p style="color: green"><%= notice %></p>
<h1>Users</h1>
<div id="users" class="flex-index-data">
  <% @users.each do |user| %>
    <%= render user %>
    <%= link_to "Edit this user", edit_admin_user_path(user) %>
    <%= link_to "Destroy this user", admin_user_path(user), method: :delete %>
  <% end %>
</div>
<%= link_to "New user", new_admin_user_path %>
