<div id="<%= dom_id user %>">
  <p>
    <strong>First name:</strong>
    <%= user.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= user.last_name %>
  </p>

  <p>
    <strong>Username:</strong>
    <%= user.username %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= user.email %>
  </p>

  <p>
    <strong>User type:</strong>
    <%= user.user_type_id %>
  </p>

  <p>
    <strong>Handicap:</strong>
    <%= user.handicap %>
  </p>

  <p>
    <strong>Street address:</strong>
    <%= user.street_address %>
  </p>

  <p>
    <strong>Address locatity:</strong>
    <%= user.address_locatity %>
  </p>

  <p>
    <strong>Address region:</strong>
    <%= user.address_region %>
  </p>

  <p>
    <strong>Postal:</strong>
    <%= user.postal_code %>
  </p>


</div>
